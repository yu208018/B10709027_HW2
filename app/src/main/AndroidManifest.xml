<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.waitlist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.android.B10709027_HW2.SettingsFragment" />
        <activity android:name="com.example.android.B10709027_HW2.AddActivity" />
        <activity android:name="com.example.android.B10709027_HW2.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.android.B10709027_HW2.SettingActivity"
            android:label="Settings"
            android:parentActivityName="com.example.android.B10709027_HW2.MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.B10709027_HW2.MainActivity" />
        </activity>
    </application>

</manifest>